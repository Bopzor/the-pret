{
  "expo": {
    "name": "tea-time",
    "slug": "tea-time",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "/src/infrastructure/App.tsx",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bopzor.rnexpotemplate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.bopzor.rnexpotemplate"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": [
            "assets/tea_time.mp3"
          ]
        }
      ]
    ]
  }
}
